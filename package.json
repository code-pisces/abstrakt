    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "husky": "latest",
    "jest": "latest",
    "jest-styled-components": "latest",
    "lint-staged": "latest",
    "plop": "latest",
    "prettier": "latest",
    "typescript": "latest"
